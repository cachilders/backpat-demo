{
  "name": "backpat-demo",
  "version": "0.7.0",
  "description": "A demonstration app for the Backpat node module",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack/webpack-dev.config.js --watch --colors",
    "postinstall": "webpack --config ./webpack/webpack-prod.config.js",
    "build": "rm -rf dist && webpack --config ./webpack/webpack-prod.config.js --colors",
    "start": "node ./index.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack"
  },
  "keywords": [
    "Backpat"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cachilders/backpat-demo.git"
  },
  "author": "Casey A. Childers",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "jsdom": "^10.1.0",
    "mocha": "^3.4.1",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.8.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "backpat": "^0.7.0",
    "css-loader": "^0.28.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.26.0",
    "jquery": "^3.1.1",
    "node-sass": "^4.2.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^6.0.5",
    "semantic-ui-react": "^0.68.3",
    "style-loader": "^0.17.0",
